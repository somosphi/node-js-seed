{
  "name": "node-js-seed",
  "version": "2.0.0",
  "description": "JavaScript backend seed",
  "main": "src/index.js",
  "scripts": {
    "migration:up": "knex migrate:latest",
    "start": "npm run migration:up && pm2 start process.yml",
    "stop": "pm2 stop process.yml",
    "restart": "pm2 restart process.yml",
    "dev": "nodemon src/index.js",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --opts test/unit/mocha.opts 'test/unit/**/*.js'",
    "test:integration": "mocha --opts test/integration/mocha.opts 'test/integration/**/*.js'",
    "lint": "eslint src test",
    "cover": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/somosphi/node-js-seed"
  },
  "author": "4All Tecnologia EPP",
  "license": "MIT",
  "dependencies": {
    "@4alltecnologia/http-errors": "^1.1.0",
    "@fnando/cnpj": "^0.1.1",
    "@fnando/cpf": "^0.1.1",
    "@somosphi/logger": "^1.2.10",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.0",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^2.17.2",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "i18n": "^0.8.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "ms": "^2.1.2",
    "mysql2": "^1.7.0",
    "nodemon": "^2.0.1",
    "ramda": "^0.26.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  }
}
